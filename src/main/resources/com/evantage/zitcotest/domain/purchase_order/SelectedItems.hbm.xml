<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.evantage.zitcotest.domain.purchase_order.SelectedItems" table="selected_items" >
        <id name="id" type="Integer">
            <column name="id" length="30" />
            <generator class="identity" />
        </id>
         
        <property name="po_id" type="Integer">
            <column name="po_id" length="30"/>
        </property>
        <property name="supplier_id" type="Integer">
            <column name="supplier_id" length="30"/>
        </property>
        <property name="selected_item" type="String">
            <column name="selected_item" length="50"/>
        </property>
        <property name="quantity" type="Integer">
            <column name="quantity" length="30"/>
        </property>
        <property name="price" type="Integer">
            <column name="price" length="30"/>
        </property>
        
    </class>
</hibernate-mapping>
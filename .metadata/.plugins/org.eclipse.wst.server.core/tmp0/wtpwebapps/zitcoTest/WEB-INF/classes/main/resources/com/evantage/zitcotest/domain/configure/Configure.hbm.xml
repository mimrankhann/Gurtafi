<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.evantage.zitcotest.domain.configure.Configure" table="configure" >
        <id name="customerId" type="string">
            <column name="ID" length="10" not-null="true"/>
            <generator class="identity" />
        </id>
        <property name="zitcoUserName" type="string">
            <column name="zitcoUserName" length="10" not-null="true" />
        </property>
        <property name="zitcoAdminPassword" type="string">
            <column name="zitcoAdminPassword" length="20" not-null="true" />
        </property>
        
        
    </class>
</hibernate-mapping>
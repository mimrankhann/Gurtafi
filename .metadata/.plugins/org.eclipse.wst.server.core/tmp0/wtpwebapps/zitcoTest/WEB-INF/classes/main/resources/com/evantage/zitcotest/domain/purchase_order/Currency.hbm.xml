<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.evantage.zitcotest.domain.purchase_order.Currency" table="currency" >
      
      
        <id name="currency_id" type="Integer">
            <column name="id" length="30" />
        </id>
          
        <property name="currency_name" type="String">
            <column name="currency_name" length="50"/>
        </property>
        
        <property name="currency_sign" type="String">
            <column name="currency_sign" length="50"/>
        </property>
         
    </class>
</hibernate-mapping>